{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/app/main(.*)",
        "dest": "index.js"
      }
    ],
    "redirects" : [
      {
        "source" : "/app/main",
        "destination" : "index.js"
      }
    ]
}