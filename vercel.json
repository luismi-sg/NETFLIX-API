{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "https://netflix-proyecto.vercel.app/app/main(.*)",
        "dest": "/"
      }
    ]
}